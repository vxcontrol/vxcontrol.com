        "@types/node": "^16.18.3",
        "@types/react": "^18.0.25",
        "@types/react-dom": "^18.0.8",
        "autoprefixer": "^10.4.13",
        "classnames": "^2.3.2",
        "eslint": "^8.27.0",
        "eslint-config-next": "^13.0.3",
        "eslint-config-prettier": "^8.5.0",
        "husky": "^8.0.2",
        "lint-staged": "^13.0.3",
        "postcss": "^8.4.31",
        "prettier": "^2.7.1",
        "prettier-plugin-tailwindcss": "^0.1.13",
        "tailwindcss": "^3.2.4",
        "typescript": "4.7.4"
    }
